{
  "name": "mysql8-schema-ts",
  "version": "2.0.0",
  "description": "mysql8-schema-ts coverts mysql schemas into typescript interfaces",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "author": "Netto Farah <nettofarah@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "docker-compose up -d && jest --runInBand --forceExit",
    "pub": "tsc && np",
    "mycli": "docker run -it --rm --network host chaifeng/mycli mysql://localhost:3306/test",
    "cli": "ts-node --project ./tsconfig.json --files ./src/index.ts"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rasmussyberg/mysql8-schema-ts.git"
  },
  "dependencies": {
    "camelcase": "^6.2.0",
    "lodash": "^4.17.20",
    "meow": "^8.0.0",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "sql-template-strings": "^2.2.2"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "mysql-schema-ts": "./dist/dist/index.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{yml,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "typescript",
    "sql",
    "schema"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "@types/lodash": "^4.14.165",
    "@types/meow": "^5.0.0",
    "@types/node": "^14.14.10",
    "@types/prettier": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^7.0.0",
    "prettier": "^2.2.1",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "np": "^7.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
}
